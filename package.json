{
  "name": "antd-schema-form-website-v4",
  "description": "antd-schema-form的在线演示地址的源代码。",
  "scripts": {
    "dll": "milktea-esm dll",
    "start": "cross-env NODE_ENV=development milktea-esm start --server",
    "build": "npm run clean && cross-env NODE_ENV=production milktea-esm build",
    "clean": "rimraf ./dist",
    "server": "milktea-esm server",
    "eslint": "cross-env COMMITLINT=1 eslint ./ --fix --cache --cache-location .eslintcache",
    "eslint:no-fix": "cross-env COMMITLINT=1 eslint ./ --cache --cache-location .eslintcache",
    "update-version": "milktea-esm update",
    "postinstall": "husky install"
  },
  "packageManager": "yarn@3.2.4",
  "dependencies": {
    "@ant-design/icons": "^5.4.0",
    "@reduxjs/toolkit": "^2.2.7",
    "antd": "^5.20.1",
    "antd-schema-form": "^5.0.1",
    "classnames": "^2.5.1",
    "copy-to-clipboard": "^3.3.3",
    "dayjs": "^1.11.12",
    "highlight.js": "^11.10.0",
    "lodash-es": "^4.17.21",
    "monaco-editor": "^0.50.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
    "react-monaco-editor": "^0.56.1",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.26.0",
    "reselect": "^5.1.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.23.3",
    "@eslint/compat": "^1.1.1",
    "@sweet-milktea/milktea": "^4.3.0",
    "@sweet-milktea/milktea-cli": "^4.1.0",
    "@sweet-milktea/server": "^4.3.0",
    "@sweet-milktea/util-tools": "^4.1.0",
    "babel-plugin-import-components-style": "^1.0.0",
    "commitizen": "^4.3.0",
    "copy-webpack-plugin": "^12.0.2",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.9.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.35.0",
    "globals": "^15.8.0",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.9",
    "monaco-editor-webpack-plugin": "^7.1.0",
    "rimraf": "^6.0.1"
  },
  "lint-staged": {
    "**/*.{js,jsx,mjs,cjs}": "cross-env COMMITLINT=1 eslint --cache --cache-location .eslintcache --no-warn-ignored"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
