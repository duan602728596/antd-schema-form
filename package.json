{
  "name": "antd-schema-form-website-v4",
  "description": "antd-schema-form的在线演示地址的源代码。",
  "scripts": {
    "dll": "milktea dll",
    "start": "cross-env NODE_ENV=development milktea start --server",
    "build": "npm run clean && cross-env NODE_ENV=production milktea build",
    "clean": "rimraf ./dist",
    "server": "milktea server",
    "eslint": "eslint ./ --fix",
    "eslint:fix": "eslint ./",
    "updateVersion": "milktea update"
  },
  "dependencies": {
    "@ant-design/icons": "^4.5.0",
    "@reduxjs/toolkit": "^1.5.0",
    "antd": "^4.14.0",
    "antd-schema-form": "^4.2.0",
    "classnames": "^2.2.6",
    "copy-to-clipboard": "^3.3.1",
    "dayjs": "^1.10.4",
    "highlight.js": "^10.6.0",
    "history": "^5.0.0",
    "lodash-es": "^4.17.21",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-monaco-editor": "^0.43.0",
    "react-redux": "^7.2.2",
    "react-router": "^6.0.0-beta.0",
    "react-router-dom": "^6.0.0-beta.0",
    "reselect": "^4.0.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.13.10",
    "@sweet-milktea/milktea": "^2.2.1",
    "@sweet-milktea/milktea-cli": "^2.2.0",
    "@sweet-milktea/server": "^2.2.1",
    "@sweet-milktea/util-tools": "^0.16.4",
    "antd-dayjs-webpack-plugin": "^1.0.6",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-import-components-style": "^0.1.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "commitizen": "^4.2.3",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.22.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react": "^7.22.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "monaco-editor-webpack-plugin": "^3.0.1",
    "rimraf": "^3.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx}": "eslint"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
