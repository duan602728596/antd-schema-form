{
  "id": "$root/properties/number",
  "type": "object",
  "title": "数字",
  "description": "渲染数字类型的组件。",
  "properties": {
    "default": {
      "id": "$root/properties/number/properties/default",
      "type": "number",
      "title": "默认组件",
      "description": "默认组件"
    },
    "hasValue": {
      "id": "$root/properties/number/properties/hasValue",
      "type": "number",
      "title": "组件有默认值",
      "description": "组件有默认值",
      "$defaultValue": 10000000
    },
    "readOnly": {
      "id": "$root/properties/number/properties/readOnly",
      "type": "number",
      "title": "组件只读",
      "description": "组件只读",
      "$defaultValue": 89.64,
      "$readOnly": true
    },
    "required": {
      "id": "$root/properties/number/properties/required",
      "type": "number",
      "title": "表单必填",
      "description": "表单必填",
      "$required": true
    },
    "enum": {
      "id": "$root/properties/number/properties/enum",
      "type": "number",
      "title": "枚举",
      "description": "枚举",
      "enum": [
        11,
        22
      ],
      "$placeholder": "11, 22"
    },
    "integer": {
      "id": "$root/properties/number/properties/integer",
      "type": "integer",
      "title": "整数",
      "description": "整数"
    },
    "minimum": {
      "id": "$root/properties/number/properties/minimum",
      "type": "number",
      "title": "最小值",
      "description": "最小值",
      "minimum": 5,
      "$placeholder": "最小值为5"
    },
    "maximum": {
      "id": "$root/properties/number/properties/maximum",
      "type": "number",
      "title": "最大值",
      "description": "最大值",
      "maximum": 800.5,
      "$placeholder": "最大值为800.5"
    },
    "radio": {
      "id": "$root/properties/number/properties/radio",
      "type": "number",
      "title": "单选框组件",
      "description": "单选框组件",
      "$componentType": "radio",
      "emum": [
        1,
        2
      ],
      "enumNames": [
        "选项1",
        "选项2"
      ]
    }
  }
}