{
  "name": "antd-schema-form-website-v4",
  "description": "antd-schema-form的在线演示地址的源代码。",
  "scripts": {
    "dll": "milktea-esm dll",
    "start": "cross-env NODE_ENV=development milktea-esm start --server",
    "build": "npm run clean && cross-env NODE_ENV=production milktea-esm build",
    "clean": "rimraf ./dist",
    "server": "milktea-esm server",
    "eslint": "eslint ./ --fix",
    "eslint:no-fix": "eslint ./",
    "update-version": "milktea-esm update",
    "postinstall": "husky install"
  },
  "packageManager": "yarn@3.2.4",
  "engines": {
    "pm": "yarn@^3.2.4"
  },
  "dependencies": {
    "@ant-design/icons": "^4.8.0",
    "@reduxjs/toolkit": "^1.9.0",
    "antd": "^5.0.3",
    "antd-schema-form": "^5.0.0",
    "classnames": "^2.3.2",
    "copy-to-clipboard": "^3.3.3",
    "dayjs": "^1.11.6",
    "highlight.js": "^11.7.0",
    "lodash-es": "^4.17.21",
    "monaco-editor": "^0.34.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-monaco-editor": "^0.51.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.4.4",
    "reselect": "^4.1.7"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@sweet-milktea/milktea": "^4.0.0",
    "@sweet-milktea/milktea-cli": "^4.1.0",
    "@sweet-milktea/server": "^4.0.0",
    "@sweet-milktea/util-tools": "^4.1.0",
    "babel-plugin-import-components-style": "^1.0.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "commitizen": "^4.2.5",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.29.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.31.11",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.4",
    "monaco-editor-webpack-plugin": "^7.0.1",
    "rimraf": "^3.0.2"
  },
  "lint-staged": {
    "**/*.{js,jsx,mjs,cjs}": "eslint"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
