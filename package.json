{
  "name": "antd-schema-form-website-v4",
  "description": "antd-schema-form的在线演示地址的源代码。",
  "scripts": {
    "dll": "milktea dll",
    "start": "cross-env NODE_ENV=development milktea start --server",
    "build": "npm run clean && cross-env NODE_ENV=production milktea build",
    "clean": "rimraf ./dist",
    "server": "milktea server",
    "eslint": "eslint ./ --fix",
    "eslint:no-fix": "eslint ./",
    "update-version": "milktea update",
    "prepare": "husky install"
  },
  "packageManager": "yarn@3.1.0",
  "engines": {
    "pm": "yarn@^3.1.0"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@reduxjs/toolkit": "^1.7.0",
    "antd": "^4.17.3",
    "antd-schema-form": "^4.4.0",
    "classnames": "^2.3.1",
    "copy-to-clipboard": "^3.3.1",
    "dayjs": "^1.10.7",
    "highlight.js": "^11.3.1",
    "lodash-es": "^4.17.21",
    "prop-types": "^15.7.2",
    "react": "rc",
    "react-dom": "rc",
    "react-helmet": "^6.1.0",
    "react-monaco-editor": "^0.46.0",
    "react-redux": "^8.0.0-beta.1",
    "react-router-dom": "^6.1.1",
    "reselect": "^4.1.5"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.5",
    "@sweet-milktea/milktea": "^3.1.7",
    "@sweet-milktea/milktea-cli": "^3.1.1",
    "@sweet-milktea/server": "^3.1.3",
    "@sweet-milktea/util-tools": "^3.1.6",
    "antd-dayjs-webpack-plugin": "^1.0.6",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-import-components-style": "^0.1.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "commitizen": "^4.2.4",
    "copy-webpack-plugin": "^10.1.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.4.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-react": "^7.27.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.2",
    "monaco-editor-webpack-plugin": "^4.2.0",
    "rimraf": "^3.0.2"
  },
  "lint-staged": {
    "**/*.{js,jsx,mjs,cjs}": "eslint"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
